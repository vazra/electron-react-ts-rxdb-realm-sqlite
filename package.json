    "@types/react": "^17.0.38",
    "@types/react-bootstrap-table-next": "4.0.17",
    "@types/react-bootstrap-table2-paginator": "2.1.2",
    "@types/react-dom": "^17.0.11",
    "electron": "16.0.7",
    "electron-builder": "^22.14.5",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "typescript": "^4.5.4",
    "webpack": "~5.66.0"
