    "@types/react": "^17.0.27",
    "@types/react-bootstrap-table-next": "4.0.10",
    "@types/react-bootstrap-table2-paginator": "2.1.1",
    "@types/react-dom": "^17.0.0",
    "electron": "15.0.0",
    "electron-builder": "^22.9.1",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "typescript": "^4.1.3",
    "webpack": "~4.46.0"
