    "@types/react": "^16.9.44",
    "@types/react-bootstrap-table-next": "4.0.5",
    "@types/react-bootstrap-table2-paginator": "2.1.1",
    "@types/react-dom": "^16.9.8",
    "electron": "9.1.2",
    "electron-builder": "^22.8.0",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "typescript": "^3.9.7",
    "webpack": "~4.44.1"
